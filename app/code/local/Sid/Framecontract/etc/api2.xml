<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE config>
<config>
    <api2>
        <resource_groups>
            <framecontract translate="title" module="framecontract">
                <title>Rahmenvertrag</title>
                <sort_order>10</sort_order>
            </framecontract>
        </resource_groups>
 
        <resources>
            <framecontract_los translate="title" module="framecontract">
                <group>framecontract</group>
                <model>framecontract/api2_los</model>
                <title>Los</title>
                <sort_order>10</sort_order>
 
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>

                <attributes translate="title" module="framecontract">
                    <los_id>Los Id</los_id>
	                <framecontract_contract_id> Contract Id</framecontract_contract_id>
	                <title>Title</title>
	                <note>Node</note>
	                <status>Status</status>
	                <key>Key</key>
	                <link_valid_to>Link Valid</link_valid_to>
                </attributes>
                <routes>
                    <route_entity>
                        <route>/framecontract/los/:ident</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </framecontract_los>
            
           <framecontract_contract translate="title" module="framecontract">
                <group>framecontract</group>
                <model>framecontract/api2_contract</model>
                <title>Vertrag</title>
                <sort_order>10</sort_order>
 
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
 
                <attributes translate="title" module="framecontract">
                    <framecontract_contract_id>Contract Id</framecontract_contract_id>
                    <title>Title</title>
                    <framecontract_vendor_id>Vendor Id</framecontract_vendor_id>
                    <operator>Operator</operator>
                    <order_email>Order Email</order_email>

                    <status>Status</status>
                    <start_date>Start Date</start_date>
                    <end_date>End Date</end_date>
                    <created_time>Created Date</created_time>
                    <update_time>Updated Date</update_time>
                    <contractnumber>Contract Number</contractnumber>
                    <store_id>Store Id</store_id>

                    <contract_manager>Contract Manager</contract_manager>
                </attributes>

                <routes>
                    <route_entity>
                        <route>/framecontract/contract/:id</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </framecontract_contract>
            
            
            <framecontract_vendor translate="title" module="framecontract">
                <group>framecontract</group>
                <model>framecontract/api2_vendor</model>
                <title>Lieferant</title>
                <sort_order>10</sort_order>
 
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
 
                <attributes translate="company" module="framecontract">
                    <framecontract_vendor_id>Vendor Id</framecontract_vendor_id>
                    <company>Company</company>
                    <operator>Operator</operator>
                    <order_email>Order Email</order_email>
                    <email>Email</email>
                    <street>Street</street>
                    <city>City</city>
                    <plz>PLZ</plz>
                    <fax>Fax</fax>
                    <tel>Tel</tel>
                    <country>Country</country>
                    <created_time>Created Time</created_time>
                    <update_time>Updated Time</update_time>
                    <claim_email>Claim Email</claim_email>
	            </attributes>

                <routes>
                    <route_entity>
                        <route>/framecontract/vendor/:id</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </framecontract_vendor>
        </resources>
    </api2>
</config>