<?xml version="1.0" encoding="UTF-8"?>
<config>
    <api2>
        <resource_groups>
            <framecontract translate="title" module="framecontract">
                <title>Rahmenvertrag</title>
                <sort_order>10</sort_order>
            </framecontract>
        </resource_groups>
 
        <resources>
            <framecontract_los translate="title" module="framecontract">
                <group>framecontract</group>
                <model>framecontract/api2_los</model>
                <title>Los</title>
                <sort_order>10</sort_order>
 
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
 <!-- 
                <attributes translate="title" module="framecontract">
                    <entity_id>Product ID</entity_id>
                    <name>Name</name>
                    <price>Preis</price>
                    <sku>SKU</sku>
                </attributes>
  -->
                <routes>
                    <route_entity>
                        <route>/framecontract/los/:ident</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </framecontract_los>
            
           <framecontract_contract translate="title" module="framecontract">
                <group>framecontract</group>
                <model>framecontract/api2_contract</model>
                <title>Vertrag</title>
                <sort_order>10</sort_order>
 
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
 <!-- 
                <attributes translate="title" module="framecontract">
                    <entity_id>Product ID</entity_id>
                    <name>Name</name>
                    <price>Preis</price>
                    <sku>SKU</sku>
                </attributes>
  -->
                <routes>
                    <route_entity>
                        <route>/framecontract/contract/:id</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </framecontract_contract>
            
            
            <framecontract_vendor translate="title" module="framecontract">
                <group>framecontract</group>
                <model>framecontract/api2_vendor</model>
                <title>Lieferant</title>
                <sort_order>10</sort_order>
 
                <privileges>
                    <admin>
                        <retrieve>1</retrieve>
                    </admin>
                </privileges>
 <!-- 
                <attributes translate="title" module="framecontract">
                    <entity_id>Product ID</entity_id>
                    <name>Name</name>
                    <price>Preis</price>
                    <sku>SKU</sku>
                </attributes>
  -->
                <routes>
                    <route_entity>
                        <route>/framecontract/vendor/:id</route>
                        <action_type>entity</action_type>
                    </route_entity>
                </routes>
                <versions>1</versions>
            </framecontract_vendor>
        </resources>
    </api2>
</config>