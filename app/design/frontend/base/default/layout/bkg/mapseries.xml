<layout version="0.1.0">
    <PRODUCT_TYPE_mapseries translate="label" module="catalog">
        <label>Catalog Product View (Grouped)</label>
         <reference name="head">
         
            <action method="addJs"><script>bkg/ol.buttonctrl.js</script></action>
            <action method="addJs"><script>bkg/helper.js</script></action>
            <action method="addJs"><script>bkg/mapseries.js</script></action>
            <action method="addItem"><type>js</type><name>lib/jquery/loadingoverlay.js</name></action>
            <action method="addItem"><type>skin_js</type><name>js/bkg/color.js</name></action>

         <!-- 
            <action method="addJs"><script>bkg/mapseries/view.js</script></action>
         -->
         </reference>
        <reference name="product.info">
            <block type="mapseries/product_view_type_mapseries" name="product.info.grouped" as="product_type_data" template="catalog/product/view/type/mapseries.phtml">
                <block type="core/text_list" name="product.info.grouped.extra" as="product_type_data_extra" translate="label">
                    <label>Product Extra Info</label>
                </block>
               <block type="mapseries/product_view_type_mapseries_map" name="map" />
            </block>
        </reference>
    </PRODUCT_TYPE_mapseries>
</layout>
