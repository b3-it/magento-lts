<?xml version="1.0"?>
<!DOCTYPE layout>
<layout version="0.1.0">
    <adminhtml_ibewi_access_index>
        <reference name="content">
            <block type="ibewi/adminhtml_access" name="ibewi" />
        </reference>
    </adminhtml_ibewi_access_index>
   <adminhtml_ibewi_customer_index>
        <reference name="content">
        	<block type="ibewi/adminhtml_container" name="ibewi" template="dwd/ibewi/container.phtml">
        		<block type="ibewi/adminhtml_filter" as="top" name="top" />
            	<block type="ibewi/adminhtml_customer" as="grid" name="grid" />
        	</block>
        </reference>
    </adminhtml_ibewi_customer_index>
    <adminhtml_ibewi_order_index>
        <reference name="content">
        	<block type="ibewi/adminhtml_container" name="ibewi" template="dwd/ibewi/container.phtml">
            	<block type="ibewi/adminhtml_filter" as="top" name="top" />
            	<block type="ibewi/adminhtml_order" as="grid" name="grid" />
            </block>
        </reference>
    </adminhtml_ibewi_order_index>
    
   <adminhtml_ibewi_report_attribute_index>
        <reference name="content">
            <block type="ibewi/adminhtml_report_attribute" name="ibewi" >
                 <block type="adminhtml/store_switcher" name="store_switcher" as="store_switcher">
                    <action method="setUseConfirm"><params>0</params></action>
                </block>
            </block>
        </reference>
    </adminhtml_ibewi_report_attribute_index>
    <adminhtml_ibewi_kostentraeger_attribute_index>
        <reference name="content">
            <block type="ibewi/adminhtml_kostentraeger_attribute" name="kostentraeger_attribute" />
        </reference>
    </adminhtml_ibewi_kostentraeger_attribute_index>
 
</layout>