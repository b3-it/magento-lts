<layout>
    <adminhtml_sidreport_sales_sales>
        <reference name="content">
            <block type="sidreport/adminhtml_sales" template="report/grid/container.phtml" name="sales.report.grid.container">
                <block type="adminhtml/store_switcher" template="report/store/switcher/enhanced.phtml" name="store.switcher">
                    <action method="setStoreVarName"><var_name>store_ids</var_name></action>
                </block>
                <block type="sidreport/adminhtml_sales_filter_form_order" name="grid.filter.form">
                    
                </block>
            </block>
        </reference>
    </adminhtml_sidreport_sales_sales>
    <adminhtml_sidreport_product_index>
        <reference name="content">
            <block type="sidreport/adminhtml_product" name="report" />
        </reference>
    </adminhtml_sidreport_product_index>
    
</layout>
