<?xml version="1.0"?>
<!DOCTYPE layout>
<layout version="0.1.0">
    <adminhtml_framecontract_contract_index>
        <reference name="content">
            <block type="framecontract/adminhtml_contract" name="framecontract" />
        </reference>
    </adminhtml_framecontract_contract_index>
    <adminhtml_framecontract_vendor_index>
        <reference name="content">
            <block type="framecontract/adminhtml_vendor" name="framecontract" />
        </reference>
    </adminhtml_framecontract_vendor_index>
    <adminhtml_framecontract_vendor_edit>
        <reference name="head">
            <action method="addJs"><script>sid/framecontract/vendor.js</script></action>
        </reference>
    </adminhtml_framecontract_vendor_edit>
    <adminhtml_framecontract_product_index>
        <reference name="content">
            <block type="framecontract/adminhtml_product" name="framecontract" />
        </reference>
    </adminhtml_framecontract_product_index>
    <adminhtml_framecontract_order_index>
        <reference name="content">
            <block type="framecontract/adminhtml_order" name="framecontract" />
        </reference>
    </adminhtml_framecontract_order_index>
    <adminhtml_framecontract_import_index>
    <update handle="jquery_load_handle"/>
    	 <reference name="head">
          <action method="addJs"><script>lib/jquery/uploadify/jquery.uploadify.min.js</script></action>
          <action method="addCss"><stylesheet>lib/jquery/uploadify/uploadify.css</stylesheet></action>
        </reference>
        <reference name="content">
            <block type="adminhtml/template" template="importexport/import/form/before.phtml" name="import.form.before" alias="form_before"/>
            <block type="framecontract/adminhtml_import_edit" name="import.form.container"/>
            <block type="adminhtml/template" template="importexport/import/form/after.phtml" name="import.form.after" alias="form_after"/>
        </reference>
    </adminhtml_framecontract_import_index>
    <adminhtml_framecontract_import_validate>
        <block type="framecontract/adminhtml_import_frame_result" template="importexport/import/frame/result.phtml" name="import.frame.result" alias="import_frame_result" output="toHtml"/>
    </adminhtml_framecontract_import_validate>
    <adminhtml_framecontract_import_start>
        <block type="framecontract/adminhtml_import_frame_result" template="importexport/import/frame/result.phtml" name="import.frame.result" alias="import_frame_result" output="toHtml"/>
    </adminhtml_framecontract_import_start>
    <adminhtml_framecontract_import_busy>
        <reference name="content">
            <block type="core/template" template="importexport/busy.phtml" name="busy" alias="busy"/>
        </reference>
    </adminhtml_framecontract_import_busy>
</layout>