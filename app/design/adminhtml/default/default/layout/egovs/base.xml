<?xml version="1.0"?>
<layout version="0.1.0">
    <default>
        <reference name="head">
            <action method="addCss"><stylesheet>css/egovs_default.css</stylesheet></action>
            <action method="addCss" ifconfig="admin/design/mode_test"><stylesheet>css/shop-state-test.css</stylesheet> </action>
            <action method="addCss" ifconfig="admin/design/mode_int"><stylesheet>css/shop-state-int.css</stylesheet> </action>
            <action method="addCss" ifconfig="admin/design/mode_prod"><stylesheet>css/shop-state-prod.css</stylesheet> </action>
            <!-- Muss an letzter Stelle stehen um als DEFAULT zu gelten -->
            <action method="addCss" ifconfig="admin/design/mode_default"><stylesheet>css/shop-state.css</stylesheet> </action>
            
            <action method="addJs"><script>egovs/jquery.admin.js</script></action>
        </reference>
    </default>
    <adminhtml_egovsbase_tools_analyse_sales_order_item_index>
        <reference name="content">
            <block type="egovsbase/adminhtml_tools_analyse_sales_order_item" name="egovssbase" >
            </block>
        </reference>
    </adminhtml_egovsbase_tools_analyse_sales_order_item_index>

